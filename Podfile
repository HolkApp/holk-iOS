target 'Holk' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire'
  pod 'SwiftLint'

  # Reactive Programming
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxAlamofire'

  target 'HolkTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
